{
  "name": "top-state-hook",
  "version": "0.3.1",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvolkmann/top-state-hook.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "babel src -d lib",
    "format": "prettier --write 'src/**/*.{js,scss}'",
    "lint": "eslint --quiet src --ext .js",
    "prepublish": "babel src -d lib",
    "prepush": "npm run verify",
    "test": "react-scripts test",
    "test-no-watch": "cross-env CI=true react-scripts test --env=jsdom",
    "verify": "npm-run-all format lint",
    "verify-save": "npm-run-all format lint test-no-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-scripts": "2.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react-testing-library": "^5.5.4"
  }
}
